{
  "name": "weex-rx-webpack-plugin",
  "main": "lib/index",
  "description": "weex webpack plugin for Rx framework",
  "version": "0.2.0",
  "licenses": "GPL-3.0",
  "keyword": [
    "weex",
    "webpack",
    "plugin",
    "webpack-rx-plugin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://git@github.com:weexteam/weex-rx-webpack-plugin.git"
  },
  "scripts": {
    "prepublish": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "coverage": "NODE_ENV=test npm test && nyc report --reporter=text-lcov | coveralls",
    "pretest": "npm run prepublish",
    "test": "nyc mocha"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "~0.1.2",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "~6.5.0",
    "babel-preset-stage-0": "~6.5.0",
    "babel-register": "^6.16.3",
    "coveralls": "^2.11.6",
    "eslint": "^2.13.1",
    "expect": "^1.20.2",
    "mocha": "^2.4.5",
    "nyc": "^7.0.0",
    "webpack": "~1.13.0"
  },
  "author": {
    "name": "hugohua",
    "email": "baofen14787@gmail.com"
  },
  "maintainers": [
    {
      "name": "hugohua",
      "email": "baofen14787@gmail.com"
    },
    {
      "name": "yuanyan",
      "email": "yuanyan.cao@gmail.com"
    }
  ],
  "readmeFilename": "README.md",
  "peerDependencies": {
    "webpack": "~1.13.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "add-module-exports"
    ]
  }
}
